FullName\"\r\n                                label=\"Personel\"\r\n                                rules={[\r\n                                    {\r\n                                        required: true,\r\n                                        message: \"Lütfen Personel Adını Giriniz\",\r\n                                    },\r\n                                ]}\r\n                            >\r\n                                <Input placeholder=\"Personel Ad Soyadı\" />\r\n                            </Form.Item>\r\n\r\n                            <Form.Item label=\"Aktiflik\" name=\"isActive\" valuePropName=\"checked\">\r\n                                <Switch />\r\n                            </Form.Item>\r\n\r\n                            <Form.Item {...tailFormItemLayout}>\r\n                                <Button type=\"primary\" loading={loading} htmlType=\"submit\">\r\n                                    Kaydet\r\n                                </Button>\r\n                            </Form.Item>\r\n                        </Form>\r\n                    </Card>\r\n                </Col>\r\n            </Row>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default SaveSaloonPersons;\r\n",{"ruleId":"171","replacedBy":"172"},{"ruleId":"173","replacedBy":"174"},{"ruleId":"175","severity":1,"message":"176","line":30,"column":8,"nodeType":"177","messageId":"178","endLine":30,"endColumn":32},{"ruleId":"175","severity":1,"message":"179","line":4,"column":8,"nodeType":"177","messageId":"178","endLine":4,"endColumn":22},{"ruleId":"180","severity":1,"message":"181","line":87,"column":6,"nodeType":"182","endLine":95,"endColumn":4,"suggestions":"183"},{"ruleId":"175","severity":1,"message":"184","line":11,"column":9,"nodeType":"177","messageId":"178","endLine":11,"endColumn":13},{"ruleId":"175","severity":1,"message":"185","line":18,"column":11,"nodeType":"177","messageId":"178","endLine":18,"endColumn":18},{"ruleId":"180","severity":1,"message":"186","l